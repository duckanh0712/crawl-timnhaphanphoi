#!/usr/bin/env groovy

node {
    properties([disableConcurrentBuilds()])

    try {

        stage ("checkout code"){
            echo "checkout code da"
        }
           stage ("build"){
            echo " build code da"
        }
           stage ("push code"){
            echo "push code da"
        }
           stage ("push docker-compose file"){
            echo "push docker-compose Lon me jenkins"
        }

    } catch (e) {
      
        throw e
    }
}
